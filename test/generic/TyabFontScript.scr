intarray TyFnt3x5   # actual font

# PY1, PX1 = lower left corner of draw
# PY2, PX2 = upper right corner of draw
# PY3, PY4 = location of font upper left point
# PI01 = number to draw 0 - 9, or 10-15 for A-F
# PI02 = off tileid (0 to not draw off tiles)
# PI03 = on tileid  (0 to not draw on tiles)
# PI04 = row DY per pixel (-1,0,1)
# PI05 = row DX per pixel (-1,0,1)
# PI06 = col DY per pixel (-1,0,1)
# PI07 = col DX per pixel (-1,0,1)
# callers should have read in tiles if using 0 id's
#
TyabDraw3x5Value::TI07=PI01*15;           # index into character data
TI03=0;                 # relative Y change each row
TI04=0;                 # relative X change each row
L01=0;                  # row loop
TyDr3x5YL;              # draw the character
# - queue draw the result
TI01=PI02;
((TyLavaOrWater[TI01]==1))TyFntDrawQueue; # background is water or lava
TI01=PI03;
((TyLavaOrWater[TI01]==1))TyFntDrawQueue;  # foreground is water or lava
TI02=TyLavaOrWater[PI02]+TyLavaOrWater[PI03];
TI01=TyNeg1;               # everything but water and lava
((TI02==0))[TyFntDraw][TyFntMaybeDraw];

TyFntMaybeDraw::((TI02<2))TyFntDrawQueue;  # one of the values is not water or lava so draw it

TyFntDrawQueue::RI02=PI01;   # save PI01
PI01=TI01;                   #tile type to draw
TyabQueueDrawRect;           # queue draw
PI01=RI02;                   #restore PI01

TyFntDraw::RI02=PI01;   # save PI01
PI01=0;                 # tile type to draw
TyabDrawRect;           # queue draw
PI01=RI02;              # restore PI01

TyDr3x5YL::L02=0;       # col loop
TI05=TI03;              # Y change as col loop
TI06=TI04;              # X change as col loop
TyDr3x5XL;              # loop X
TI03+=PI04;             # update relative row
TI04+=PI05;             # update relative col
L01+=1;
((L01<5))TyDr3x5YL;     # loop rows

TyDr3x5XL::((TyFnt3x5[TI07]==0))[iPI01=PI02][iPI01=PI03];  #get off or on tileid
iPY1=PY3+TI05;
iPX1=PX3+TI06;
((iPI01>0))iTyabSetTile; #only change if not 0
TI05+=PI06;             # update row per col
TI06+=PI07;             # update col per col
TI07+=1;                # next character id index
L02+=1;
((L02<3))TyDr3x5XL;     # loop rows

